{
  "name": "@helium/organization-sdk",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Apache-2.0",
  "version": "0.0.1",
  "description": "Interface to the organization smart contract",
  "repository": {
    "type": "git",
    "url": "https://github.com/HeliumFoundation/modular-governance"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "import": "./lib/esm/src/index.js",
    "require": "./lib/cjs/index.js",
    "types": "./lib/types/src/index.d.ts"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npx git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "clean": "npx shx mkdir -p lib && npx shx rm -rf lib",
    "package": "npx shx mkdir -p lib/cjs lib/esm",
    "prebuild": "npm run clean && npm run package",
    "docs": "typedoc"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@helium/modular-governance-idls": "^0.0.1",
    "@helium/proposal-sdk": "^0.0.1",
    "@helium/anchor-resolvers": "^0.2.5"
  },
  "devDependencies": {
    "git-format-staged": "^2.1.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "yarn": "^1.22.18"
  },
  "keywords": [],
  "author": ""
}
